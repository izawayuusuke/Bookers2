<h1>Books</h1>
<table>
  <thead>
  	<tr>
  	  <th></th>
  	  <th>Title</th>
  	  <th>Option</th>
  	  <th colspan="3"></th>
  	</tr>
  </thead>
  <tbody>
  	<% @books.each do |book| %>
  	  <tr>
  	    <td> <%= book.title %> </td>
  	    <td> <%= book.body %> </td>
  	  </tr>
  	<% end %>
  </tbody>
</table>